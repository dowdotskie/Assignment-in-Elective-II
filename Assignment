			<input type ="text" name="text5" id="id5" onkeyup="command()" disabled></input>
			</form>
		<center>
		<script>
			function command(){
			var z= document.getElementById("id1");
			var y= document.getElementsByTagName("INPUT");

			for(var x=0;x<y.length; x++){
			y[x].value=z.value;
			}

			}
		</script>		
